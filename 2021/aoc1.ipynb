    "import numpy as np\n",
    "\n",
    "with open(\"in1.txt\") as f:\n",
    "\tinput = f.read().splitlines()\n",
    "\n",
    "input = np.array(input,dtype=\"int32\")\n",
    "print(np.sum(np.diff(input) > 0))\n",
    "\n",
    "windows = np.convolve(input,[1,1,1],'valid')\n",
    "print(np.sum(np.diff(windows) > 0))"
   ]
  }
